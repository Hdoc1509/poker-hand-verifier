{
  "name": "poker-hand-verifier",
  "version": "1.0.0",
  "description": "Provides a verifier for a poker hand with 5 cards.",
  "main": "./cjs/index.mjs",
  "module": "./esm/index.cjs",
  "types": "types/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "files": [
    "cjs",
    "esm",
    "dist",
    "types"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.cjs"
    }
  },
  "scripts": {
    "build": "node build.js",
    "lint": "eslint --color src/**",
    "test": "jest tests src/utils --colors --verbose",
    "test-source": "jest tests --colors --verbose",
    "test-utils": "jest src/utils --colors --verbose",
    "check-dependencies": "ncu --color --packageFile package.json",
    "update-dependencies": "npm run check-dependencies -- -u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hdoc1509/poker-hand-verifier.git"
  },
  "keywords": [
    "poker",
    "poker hand",
    "evaluator",
    "verifier"
  ],
  "unpkg": "dist/poker-hand-verifier.min.js",
  "author": "Hdoc1509",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hdoc1509/poker-hand-verifier/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/Hdoc1509/poker-hand-verifier#readme",
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "esbuild": "0.14.49",
    "eslint": "8.20.0",
    "jest": "28.1.3",
    "jest-extended": "3.0.1",
    "npm-check-updates": "15.3.4",
    "ts-jest": "28.0.7",
    "typescript": "4.7.4"
  }
}
